{
  "name": "graphbrainz-extension-lastfm",
  "version": "0.0.0",
  "description": "Last.fm GraphQL extension for GraphBrainz.",
  "keywords": [
    "graphbrainz",
    "extension",
    "musicbrainz",
    "graphql",
    "last.fm",
    "lastfm"
  ],
  "author": "Brian Beck <exogen@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:docs",
    "build:docs": "babel-node scripts/build-docs.js",
    "build:lib": "babel --out-dir lib src",
    "clean": "rimraf lib",
    "format": "npm run lint:fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run clean && npm run build",
    "start": "babel-node scripts/start.js",
    "start:dev": "nodemon --exec babel-node scripts/start.js",
    "test": "npm run lint && npm run test:only",
    "test:only": "AVA_PLAYBACK=play ava",
    "test:update": "AVA_PLAYBACK=record ava --serial --update-snapshots && npm run test:fixtures",
    "test:fixtures": "babel-node scripts/fix-test-fixtures.js"
  },
  "peerDependencies": {
    "graphbrainz": "^7.1.0"
  },
  "dependencies": {
    "country-list": "^1.1.0",
    "dataloader": "^1.3.0",
    "dateformat": "^3.0.2",
    "debug": "^3.1.0",
    "graphql-relay": "^0.5.3",
    "lru-cache": "^4.1.1",
    "qs": "^6.5.1"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "ava-playback": "^0.0.14",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "glob": "^7.1.2",
    "graphbrainz": "^7.1.0",
    "graphql": "^0.11.7",
    "graphql-markdown": "^3.2.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "dotenv/config",
      "babel-register",
      "ava-playback"
    ],
    "files": [
      "src/**/*.test.js",
      "test"
    ],
    "playbacks": "test/fixtures"
  }
}
